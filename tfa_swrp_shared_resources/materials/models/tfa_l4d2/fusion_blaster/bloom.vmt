"UnlitGeneric"
{
	$basetexture "models/tfa_l4d2/fusion_blaster/bloom"
	
	$color2 "[1 1 1]"
	$model "1"
	$nocull "1"
	$translucent "1"
	
    "Proxies"
	{
		"AnimatedTexture"
		{
		   "animatedtexturevar" "$basetexture"
		   "animatedtextureframenumvar" "$frame"
		   "animatedtextureframerate" "30"
		}
	}
}
